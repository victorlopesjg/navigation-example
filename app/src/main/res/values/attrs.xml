<?xml version="1.0" encoding="utf-8"?>
<resources>

    <attr name="validation" format="enum">
        <enum name="no_validation" value="-1" />
        <enum name="no_required" value="0" />
        <enum name="required" value="1" />
    </attr>
    <attr name="invalid_msg" format="string" />



    <declare-styleable name="GenericSlidingTabLayout">
        <attr name="tabs_padding" format="dimension" />
    </declare-styleable>

    <declare-styleable name="CircleImageView">
        <attr name="border_width" format="dimension" />
        <attr name="border_color" format="color" />
    </declare-styleable>
    <declare-styleable name="RangeBar">
        <attr name="tickCount" format="integer" />
        <attr name="tickHeight" format="dimension" />
        <attr name="barWeight" format="dimension" />
        <attr name="barColor" format="reference|color" />
        <attr name="connectingLineWeight" format="dimension" />
        <attr name="connectingLineColor" format="reference|color" />
        <attr name="thumbRadius" format="dimension" />
        <attr name="thumbImageNormal" format="reference" />
        <attr name="thumbImagePressed" format="reference" />
        <attr name="thumbColorNormal" format="reference|color" />
        <attr name="thumbColorPressed" format="reference|color" />
    </declare-styleable>
    <declare-styleable name="CircleButton">
        <attr name="cb_color" format="color" />
        <attr name="cb_pressed_ring_width" format="dimension" />
    </declare-styleable>

    <attr name="fontPath" format="string" />

    <declare-styleable name="FloatLabelEditText">
        <attr name="fitScreenWidth" format="enum">
            <enum name="full" value="1" />
            <enum name="half" value="2" />
        </attr>
        <attr name="gravity" format="enum">
            <enum name="left" value="3" />
            <enum name="right" value="5" />
            <enum name="center" value="17" />
        </attr>
        <attr name="hint" format="string" />
        <attr name="inputType" format="enum">
            <enum name="normal" value="0" />
            <enum name="textPassword" value="1" />
            <enum name="number" value="2" />
            <enum name="numberPassword" value="3" />
            <enum name="amount" value="4" />
            <enum name="groupby4" value="5" />
            <enum name="bankaccount" value="6" />
            <enum name="description" value="7" />
            <enum name="blocked" value="8" />
            <enum name="groupby4numbers" value="9" />
            <enum name="groupby4nib" value="10" />
            <enum name="groupby4iban" value="11" />
            <enum name="phonenumber" value="12" />
            <enum name="email" value="13" />
            <enum name="description_beneficiary_name" value="14"/>
            <enum name="swift" value="15"/>
        </attr>
        <attr name="text" format="string" />
        <attr name="textSize" format="dimension" />
        <attr name="textColorHintFocused" format="color" />
        <attr name="textColorHintUnFocused" format="color" />
        <attr name="textAllCaps" format="boolean" />
        <attr name="limit" format="integer" />
        <attr name="hasClear" format="boolean" />
        <attr name="hasShowPassword" format="boolean" />
        <attr name="tooltip" format="string" />
        <attr name="layoutType" format="enum">
            <enum name="normal" value="0" />
            <enum name="floating" value="1" />
            <enum name="horizontal" value="2" />
            <enum name="vertical" value="3" />
            <enum name="horizontal_description" value="4" />
            <enum name="floating_description" value="5" />
            <enum name="no_floating_public" value="6" />
        </attr>

        <attr name="validation" />
        <attr name="invalid_msg" />

    </declare-styleable>

    <declare-styleable name="CustomSwitchButton">
        <attr name="isChecked" format="boolean" />
    </declare-styleable>

    <declare-styleable name="PinBox">
        <attr name="pinInput">
            <enum name="number" value="0" />
            <enum name="text" value="1" />
        </attr>
    </declare-styleable>

    <!--<declare-styleable name="MaterialContentOverflow">-->
    <!--<attr name="buttonDrawable" format="reference" />-->
    <!--<attr name="fabButtonColor" format="reference" />-->
    <!--<attr name="contentColor" format="reference" />-->
    <!--<attr name="buttonPosition" format="enum">-->
    <!--<enum name="left" value="0" />-->
    <!--<enum name="center" value="1" />-->
    <!--<enum name="right" value="2" />-->
    <!--</attr>-->
    <!--</declare-styleable>-->

    <declare-styleable name="FloatingActionButton">
        <!-- Background for the FloatingActionButton -->
        <attr name="android:background" />
        <attr name="backgroundTint" />
        <attr name="backgroundTintMode" />

        <!-- Ripple color for the FAB. -->
        <attr name="rippleColor" format="color|reference" />
        <!-- Size for the FAB. -->
        <attr name="fabSize">
            <enum name="normal" value="0" />
            <enum name="mini" value="1" />
        </attr>
        <!-- Elevation value for the FAB -->
        <attr name="elevation" />
        <!-- TranslationZ value for the FAB when pressed-->
        <attr name="pressedTranslationZ" format="dimension|reference" />
    </declare-styleable>

    <declare-styleable name="DatePickerView">
        <attr name="descriptionDate" format="string" />
        <attr name="validation" />
        <attr name="invalid_msg" />
    </declare-styleable>

    <declare-styleable name="SpinnerView">
        <attr name="validation" />
        <attr name="invalid_msg" />
    </declare-styleable>



</resources>